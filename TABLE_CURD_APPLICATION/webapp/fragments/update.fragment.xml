<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns:form="sap.ui.layout.form">
<Dialog title="Update">
	<content>
		<form:SimpleForm>
			<form:content>
				<Label text="ID" />
				<Input id="F1" editable="false" value="{dummy>/ID}"  />
				<Label text="NAME" />
				<Input id="F2" value="{dummy>/NAME}" /> 
				<Label text="BRANCH" />
				<Input id="F3" value="{dummy>/BRANCH}" />  
				</form:content>
				</form:SimpleForm>
		</content>
		<beginButton>
			 <Button text="save" press="onSave"/>
			</beginButton>
			<endButton>
				<Button text="Cancel" press="onCancel"/>
				</endButton>
	</Dialog>
</core:FragmentDefinition>